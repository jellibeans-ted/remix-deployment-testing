{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "icons": "npx @svgr/cli --out-dir app/components/icons -- app/assets/icons",
    "icons:watch": "npm-watch icons",
    "dev:svg": "run-s icons icons:watch",
    "dev:sass": "sass --watch app/:app/",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development npm run wrangler",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run wrangler",
    "typecheck": "tsc",
    "wrangler": "wrangler pages dev ./public"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.17.1",
    "@remix-run/cloudflare-pages": "^1.17.1",
    "@remix-run/css-bundle": "^1.17.1",
    "@remix-run/react": "^1.17.1",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "i18next": "^23.2.3",
    "i18next-browser-languagedetector": "^7.0.2",
    "i18next-fs-backend": "^2.1.5",
    "i18next-http-backend": "^2.2.1",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.1",
    "remix-i18next": "^5.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.19.0",
    "@remix-run/dev": "^1.17.1",
    "@remix-run/eslint-config": "^1.17.1",
    "@svgr/cli": "^8.0.1",
    "@svgr/plugin-jsx": "^8.0.1",
    "@svgr/plugin-prettier": "^8.0.1",
    "@svgr/plugin-svgo": "^8.0.1",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.38.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "sass": "^1.63.6",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "wrangler": "^2.20.0"
  },
  "watch": {
    "icons": {
      "patterns": [
        "icons"
      ],
      "extensions": "svg",
      "quiet": false
    }
  },
  "engines": {
    "node": ">=16.13.0"
  }
}
